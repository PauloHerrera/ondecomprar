<!DOCTYPE html>
<html>   
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
        <title>@ViewBag.Title - Aqui você acha</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]> <script src="../../assets/js/html5shiv.js"></script> <script src="../../assets/js/respond.min.js"></script> <![endif]-->
        @Styles.Render("~/Public/Styles/css")
        @Scripts.Render("~/bundles/modernizr")
    </head>

    <body onload="initialize()">
        
        @RenderBody()
        
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/pagestart")
        @RenderSection("scripts", required: false)
    </body>
</html>
